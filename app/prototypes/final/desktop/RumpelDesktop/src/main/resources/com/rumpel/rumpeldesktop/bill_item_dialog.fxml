<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="673.0" prefWidth="654.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rumpel.rumpeldesktop.mvc.controllers.ItemBillDialogController">
   <center>
      <BorderPane prefHeight="290.0" prefWidth="496.0">
         <center>
            <AnchorPane prefHeight="363.0" prefWidth="621.0" BorderPane.alignment="CENTER">
               <TextField fx:id="nameField" layoutX="210.0" layoutY="33.0" prefHeight="36.0" prefWidth="318.0" promptText="%name" />
               <Label layoutX="91.0" layoutY="31.0" prefHeight="36.0" prefWidth="101.0" text="%name">
                  <font>
                     <Font size="14.0" />
                  </font>
               </Label>
               <Label layoutX="91.0" layoutY="142.0" prefHeight="36.0" prefWidth="118.0" text="%tags" textAlignment="CENTER">
                  <font>
                     <Font size="14.0" />
                  </font>
               </Label>
               <Spinner fx:id="spinnerPrice" editable="true" layoutX="209.0" layoutY="90.0" prefHeight="36.0" prefWidth="318.0" />
               <Label layoutX="91.0" layoutY="90.0" prefHeight="36.0" prefWidth="101.0" text="%price">
                  <font>
                     <Font size="14.0" />
                  </font>
               </Label>
               <ListView fx:id="tagList" editable="true" layoutX="209.0" layoutY="142.0" prefHeight="150.0" prefWidth="321.0" />
               <Button layoutX="211.0" layoutY="317.0" mnemonicParsing="false" onAction="#modifyItem" prefHeight="27.0" prefWidth="93.0" text="%edit">
                  <styleClass>
                     <String fx:value="btn" />
                     <String fx:value="btn-primary" />
                  </styleClass>
                  <effect>
                     <DropShadow spread="0.1">
                        <color>
                           <Color opacity="0.5" />
                        </color>
                     </DropShadow>
                  </effect>
               </Button>
               <Button layoutX="435.0" layoutY="317.0" mnemonicParsing="false" onAction="#insertItem" prefHeight="27.0" prefWidth="93.0" text="%insert">
                  <styleClass>
                     <String fx:value="btn" />
                     <String fx:value="btn-primary" />
                  </styleClass>
                  <effect>
                     <DropShadow spread="0.1">
                        <color>
                           <Color opacity="0.5" />
                        </color>
                     </DropShadow>
                  </effect>
               </Button>
               <TableView fx:id="tableView" focusTraversable="false" layoutX="25.0" layoutY="360.0" prefHeight="219.0" prefWidth="621.0">
                  <columns>
                     <TableColumn fx:id="nameCol" prefWidth="310.0" text="%name" />
                     <TableColumn fx:id="priceCol" prefWidth="309.0" text="%price" />
                  </columns>
                  <columnResizePolicy>
                     <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                  </columnResizePolicy>
               </TableView>
            </AnchorPane>
         </center>
      </BorderPane>
   </center>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <bottom>
      <BorderPane>
         <bottom>
            <BorderPane prefHeight="32.0" prefWidth="614.0" BorderPane.alignment="CENTER">
               <left>
                  <HBox alignment="CENTER_LEFT" prefHeight="32.0" spacing="15.0" BorderPane.alignment="CENTER">
                     <Label fx:id="lblTotal" text="%total_colon">
                        <font>
                           <Font name="System Bold" size="16.0" />
                        </font>
                        <opaqueInsets>
                           <Insets />
                        </opaqueInsets>
                        <padding>
                           <Insets top="15.0" />
                        </padding>
                     </Label>
                     <Label prefHeight="19.0" prefWidth="558.0" />
                  </HBox>
               </left>
            </BorderPane>
         </bottom>
      </BorderPane>
   </bottom>
</BorderPane>
